{
    "name": "first_gen",
    "cuda": true,
    "gpu": 0,
    "save_mode": "state_dict",
    "override": true,
    "super_computer":false,
    "data_loader": {
	    "QR_dataset": {
		"data_set_name": "SimpleQRDataset"
	    },
	    "image_dataset_name": "LSUN",
	    "image_class": "bedroom",
	    "image_dataset_dir": "../data/LSUN",
	    "data_dir": "none",
	    "shuffle": true,
	    "batch_size": 32,
	    "num_workers": 2
    },

    "validation": {
        "shuffle": false,
        "batch_size": 64
    },

    
    "lr_scheduler_type": "none",
 
    "optimizer_type": "Adam",
    "optimizer": {
        "lr": 0.0002,
        "weight_decay": 0,
        "betas": [0.5,0.999]
    },
    "optimizer_type_discriminator": "Adam",
    "optimizer_discriminator": {
        "lr": 0.0002,
        "weight_decay": 0,
        "betas": [0.5,0.999]
    },
    "loss": {
        "char": "cross_entropy"
    },
    "loss_weights": {
	"char": 1.0,
	"discriminatorLoss": 1.0,
	"generatorLoss": 1.0

    },
    "loss_params":
        {
        },
    "metrics": [],
    "trainer": {
        "class": "QRGenTrainer",
        "iterations": 200000,
        "save_dir": "saved/",
        "val_step": 10000,
        "save_step": 10000,
        "save_step_minor": 100,
        "log_step": 100,
        "verbosity": 1,
        "monitor": "loss",

	"freeze_param_names": ["qr_net"],
        "slow_param_names": ["keys"],
        "curriculum": {
            "0": [     
                        ["gen"],
                        ["disc"]
                    ]
        },
        "new_gen_freq": 0.8,
        "forget_new_freq": 0.95,
        "balance_loss": "sign_preserve_var",
        "balance_var_x": {"0":[1.0]},

        "use_learning_schedule": false,

        "print_dir": "train_out/example",
        "serperate_print_every": 5000,
        "print_every": 100
    },
    "arch": "QRWraper", 
    "model": {
        "generator": "ConvGen",
        "gen_dim": 8,
        "gen_use_skips": true,
	"qr": "DecoderCNN",
        "pretrained_qr": "saved/qr_net_pretrained/checkpoint-iteration100000.pth",
        "discriminator": "some disc",
        "disc_dim": 8


    }
}
