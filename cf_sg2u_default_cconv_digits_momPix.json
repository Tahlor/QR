{
    "name": "sg2u_default_cconv_digits_momPix",
    "cuda": true,
    "gpu": 0,
    "save_mode": "state_dict",
    "override": true,
    "super_computer":false,
    "data_loader": {
	    "data_set_name": "ImageAndQRDataset",
	    "QR_dataset": {
		"data_set_name": "SimpleQRDataset",
		"final_size": 256,
                "str_len": 17,
                "alphabet": "digits"
	    },
	    "image_dataset_name": "LSUN",
	    "image_class": "bedroom",
	    "data_dir": "../data/LSUN",
	    "shuffle": true,
	    "batch_size": 28,
	    "num_workers": 3
    },

    "validation": {
        "shuffle": false,
        "batch_size": 64
    },

    
    "lr_scheduler_type": "none",
 
    "optimizer_type": "Adam",
    "optimizer": {
        "lr": 0.00016,
        "betas": [0,0.992]
    },
    "optimizer_type_discriminator": "Adam",
    "optimizer_discriminator": {
        "lr": 0.00001,
        "betas": [0,0.991]
    },
    "loss": {
        "pixel":"QRCenterPixelLoss",
        "char": "cross_entropy",
        "valid": "sigmoid_BCE_loss"
    },
    "loss_weights": {
	"discriminator": 1.0,
	"generator": 1.0,
        "gen_reg": 1.0,
        "disc_reg": 1.0,
        "pixel": 0.4,

        "char": 0.5,
        "valid": 1.0

    },
    "loss_params":
        {
            "pixel":{
                "img_size": 256,
                "qr_size": 21,
                "padding":2,
                "threshold":0.1,
                "bigger": true}
        },
    "metrics": [],
    "trainer": {
        "class": "QRGenTrainer",
        "iterations": 300000,
        "save_dir": "saved/",
        "val_step": 99990000,
        "save_step": 20000,
        "save_step_minor": 500,
        "log_step": 500,
        "verbosity": 1,
        "monitor": "loss",

	"freeze_param_names": ["qr_net"],
        "slow_param_names": ["style_emb"],

        "curriculum": {
            "0": [     
                        ["gen"],
                        ["disc"],
                        ["gen"],
                        ["disc"],
                        ["gen"],
                        ["disc"],
                        ["gen"],
                        ["disc"],
                        ["gen_reg"],
                        ["gen"],
                        ["disc"],
                        ["gen"],
                        ["disc"],
                        ["gen"],
                        ["disc"],
                        ["gen"],
                        ["disc"],
                        ["gen_reg"],
                        ["gen"],
                        ["disc"],
                        ["gen"],
                        ["disc"],
                        ["gen"],
                        ["disc"],
                        ["gen"],
                        ["disc"],
                        ["gen_reg"],
                        ["gen"],
                        ["disc"],
                        ["gen"],
                        ["disc"],
                        ["gen"],
                        ["disc"],
                        ["gen"],
                        ["disc"],
                        ["gen_reg"],
                        ["disc_reg"]
                    ]
        },
        "new_gen_freq": 0.8,
        "forget_new_freq": 0.95,
        "balance_loss": "sign_preserve_var",
        "balance_var_x": {"0":[0.6,"pixel"]},
        "combine_qr_loss": true,

        "use_learning_schedule": false,

        "print_dir": "train_out/sg2u_default_cconv_digits_momPix",
        "serperate_print_every": 5000,
        "print_every": 250,

        "modulate_pixel_loss": "momentum",
        "modulate_pixel_loss_start": 40000,
        "max_pixel_weight": 1.2,
        "min_pixel_weight": 0.4,
        "pixel_momentum_B": 0.8
    },
    "model": {
	"arch": "QRWraper", 
        "style_dim": 512,
        "generator": "SG2UGen small coordconv",
        "discriminator": "StyleGAN2 smaller",
        "pretrained_qr": "saved/default_cconv_digits/checkpoint-latest.pth"


    }
}
